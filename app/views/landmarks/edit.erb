<h1>Edit Landmark <%= @landmark.name %> <%= @landmark.year_completed %></h1>
<form action="/landmarks/<%= @landmark.id %>" method="post">
  <input type="hidden" name="_method" value="patch">
  <p>
    Landmark Name: <input type="text" name="landmark[name]" id="name">
  </p>
  <p>
    Landmark Year Completed: <input type="text" name="landmark[year_completed]" id="year_completed">
  </p>
  <p>
    <input type="submit" value="Edit Landmark">
  </p>
</form>
